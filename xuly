import pandas as pd

# Đọc dữ liệu từ file CSV
df = pd.read_csv("C:\\Users\\PC\\Downloads\\data_it\\placeView5711811782636688456.csv")

# Kiểm tra ngày đầu và ngày cuối cùng của cột date
first_date = df['date'].iloc[0]
last_date = df['date'].iloc[-1]
print("Ngày đầu tiên:", first_date)
print("Ngày cuối cùng:", last_date)

# Xóa cột _id nếu tồn tại
if '_id' in df.columns:
    df = df.drop(columns=['_id'])

# Chuyển đổi cột date để chỉ giữ lại năm-tháng-ngày
df['date'] = pd.to_datetime(df['date']).dt.date

# Lưu dữ liệu ra file mới
df.to_csv('aa_456.csv', index=False)
print("Dữ liệu đã được lưu vào file output_file.csv")
